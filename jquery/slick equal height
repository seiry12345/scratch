$('.main-slider .view-content').slick({
      slidesToShow: 1,
      slidesToScroll: 1,
      arrows: false,
      dots: true,
      infinite: true,
      adaptiveHeight: true,
      speed: 700
    });
    $('.main-slider .view-content').on('setPosition', function (event, slick) {
      slick.$slides.css('height', slick.$slideTrack.height() + 'px');
    });
    
    
    
    // set equal height to items
    .slick-slide {
    height: 100%;
  }
